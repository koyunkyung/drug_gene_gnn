romise-polyfill@6/dist/promise.min.js"></script><script src="//cdnjs.cloudflare.com/ajax/libs/fetch/2.0.3/fetch.js"></script><script src="/public/js/lib/clue.all.min.js"></script><script src="/public/js/local/scarpa/common.js"></script><script src="//cdnjs.cloudflare.com/ajax/libs/1000hz-bootstrap-validator/0.9.0/validator.min.js" integrity="sha256-GsK96iTc0mLT6IMCG1dLAR2GJb/XebZ19d3F/8KGWFk=" crossorigin="anonymous"></script><!--from header.JADE--><script src="/public/js/local/utils/header.js"></script><script src="/public/js/local/tooltips/tooltips-popovers.js"></script><script src="/public/js/local/utils/contactmodal.js"></script><script src="/public/js/local/menu/login_signup_menu.js"></script><script src="/public/js/local/menu/toolsAndMainMenu.js"></script><script>$(document).ready(function () {
    if (clue.ENV.toLowerCase() !== "production" && clue.ENV.toLowerCase() !== "prod") {
        $("#header-clue-app").text("CLUE (" + clue.ENV.toUpperCase() + ") ");
    }

    populateToolsAndMainMenu();
    $("#filechooser").change(function () {
        let file_name = document.getElementById("filechooser").value;
        if (file_name === "" || file_name === "undefined") {
            file_name = "No file chosen";
        }
        $("#filechooser_label").text(file_name.replace("C:\\fakepath\\", ""));
    })
    $(document).on('click', function (e) {
        let $clicked = $(e.target);
        if ($clicked.hasClass("tooltip_trigger")) {
            return;
        }
        else if ($('[aria-describedby]').length) {
            $('[aria-describedby]').each(function (i, el) {
                $(this).popover('hide');
            });
        }
        else if ($clicked.hasClass("toolbar-menu-header")) {
            return;
        }
        else {
            hideOtherMenus($("#foo-menu"), ".foo-class");
        }
    });
    if (window.location.pathname.indexOf("connectopedia") > -1 && window.location.pathname !== "/connectopedia_votes") {
        $("#header_search_area").show();
    }
    $('#contactModal').on('shown.bs.modal', function (e) {
    })
    $("#contactModal").on('shown.bs.modal', function (e) {
    })
    $('#send_btn').on("click", function (e) {
        e.preventDefault();
        $("#messageTextContact").text("");
    });
    $("#header-tools").on("click", function () {
        showTools();
    })
    $("#header-projects").on("click", function () {
        showProjects();
    })
    $("#header-partnering").on("click", function () {
        showPartnering();
    })
    $("#header-developer").on("click", function () {
        showDevelopers();
    })
    $("#header-help").on("click", function () {
        showHelp();
    })
    $("#header-announcement").on('click', function (evt) {
        $("#message_area").empty();
        notificationsAlert();
    });
    $('#help-dropdown-link').on('click', function (evt) {
        clue.hideAccountDropDown();
    });
    let pageTitle = "Data";
    let pageCategory = "Tools";
    let url = "/data";
    let path = url ? url : window.location.pathname;
    let color = "#6a94a0";
    if ("" && "" !== "") {
        color = "";
    }
    if (clue.ENV.toUpperCase() === "DEVELOPMENT") {
        const homepage = document.querySelector('.homepage');
        // homepage.style.cssText += 'background-color:#e6edfe';
        const header = document.querySelector('#header-bar');
        const banner_color = '#f7941f';
        header.style.cssText += 'background-color:' + banner_color;
        const header_logged_in = document.querySelector('#header-bar * .header-login');
        header_logged_in.style.cssText += 'background-color:' + banner_color;
    }
    let navBar = new NavBar(pageTitle, path, pageCategory, color);
    let loginBox = $('#header-login');
    let settingsModal = $('#header-drop-down');
    let notificationsModal = $('#notification-modal');
    let toolsModal = $('#menu-tools-modal');
    let projectsModal = $('#menu-projects-modal');
    let partneringModal = $('#menu-partnering-modal');
    let developerModal = $('#menu-developer-modal');
    let internalModal = $('#menu-internal-modal');
    let helpModal = $('#menu-help-modal');
    $('#header-menu').on('click', function (evt) {
        evt.preventDefault();
        showMainMenu();
    });
    loginBox.on('click', function (evt) {
        if (!scarpa.isLoggedIn()) {
            $('#loginModal').modal('show');
        }
    });
    const showSettingsDropDown = function () {
        var userInfo = clue.getUserInfo();
        $('#dropdown-email').text(userInfo.email);
        $('#dropdown-name').text(userInfo.fullName);
        settingsModal.toggle();
        hideOtherMenus(settingsModal);
    }
    const hideOtherMenus = function (id, cl) {
        $(".toolbar-menu").not(id).hide();
        $(".header-arrow-down").not(cl).hide();
    }
    const showNotificationDropDown = function () {
        notificationsModal.toggle();
        hideOtherMenus(notificationsModal);
    }
    const showTools = function () {
        toolsModal.toggle();
        $(".tools-arrow").toggle();
        hideOtherMenus(toolsModal, ".tools-arrow");
    }
    const showProjects = function () {
        projectsModal.toggle();
        $(".projects-arrow").toggle();
        hideOtherMenus(projectsModal, ".projects-arrow");
    }
    const showPartnering = function () {
        partneringModal.toggle();
        $(".partnering-arrow").toggle();
        hideOtherMenus(partneringModal, ".partnering-arrow");
    }
    const showDevelopers = function () {
        developerModal.toggle();
        $(".developer-arrow").toggle();
        hideOtherMenus(developerModal, ".developer-arrow");
    }
    const showHelp = function () {
        helpModal.toggle();
        $(".help-arrow").toggle();
        hideOtherMenus(helpModal, ".help-arrow");
    }
    loginBox.on('clue-login', function (event, username) {
        loginBox.text(username);
        loginBox.css('background-color', '');
        loginBox.removeClass('not-loggedin');
        loginBox.css('color', '');
        $('#header-pop-down').show();
        loginBox.on('click', function (evt) {
            showSettingsDropDown();
        });
    });
    $(".header-pop-down").on('click', function (evt) {
        showSettingsDropDown();
    });
    $('#dropdown-account-settings').on('click', function (evt) {
        evt.preventDefault();
        clue.getSettings(function (err, result) {
            $('#account-institution').text(result.institution);
            $('#account-apiKey').text(result.apiKey);
            $('#account-roles').text(result.roles);
            $('#account-details').show();
            $('#dropdown-account-settings').hide();
            $('#dropdown-hide-account-settings').show();
        });
    });
    $('#dropdown-hide-account-settings').on('click', function (evt) {
        evt.preventDefault();
        $('#account-details').hide();
        $('#dropdown-hide-account-settings').hide();
        $('#dropdown-account-settings').show();
    });
    var numModals = 0;
    $(document).on('shown.bs.modal', function () {
        numModals++;
    });
    $(document).on('hidden.bs.modal', function () {
        setTimeout(function () {
                numModals--;
            },
            200);
    });
    $(document).on('hide.bs.modal', function () {
        $('.help-block.with-errors', this).text('');
        $('.has-error').removeClass('has-error')
    });
    $(document).keyup(function (e) {
        if (e.keyCode == 27) {
            if (numModals === 0) {
                if (settingsModal.is(':visible')) {
                    settingsModal.hide();
                }
            }
        }
    });
    clue.getGitInfo(function (err, result) {
        if (result) {
            var branch = result.branch;
            var SHA = result.SHA;
            var short = result.SHORT;
            var message = "Branch: " + branch + " Build Date: " + result.DATE + " Revision: " + SHA + " Short: " + short;
            $('.footer-technical').html(message);
        }
    });
    const currentTitle = document.getElementById('cluecmap').innerHTML;
    notifications();
    document.getElementById('cluecmap').innerHTML = currentTitle + " [" + window.location.hostname + "]";
    scarpa.appLaunch();
    clue.analytics();
    function notifications() {
        $.ajax({
            url: "/events",
            type: 'GET'
        }).done(function (result) {
            var numNewMessages = result.data;
            if (numNewMessages > 0) {
                $("#notification_number").text(numNewMessages).show();
            }
            else {
                $("#notification_number").text(numNewMessages).hide();
            }
        });
    }
    function notificationsAlert() {
        const notif = clue.getAllEvents();
        notif.done(function (messages) {
            if (messages && messages.messages && messages.messages.length > 0) {
                var lastId = messages.old_event_id;
                var modallength = 10;
                if (messages.messages.length < 10) {
                    modallength = messages.messages.length;
                }
                //messages come back in descending order, recent first
                for (var i = 0; i < modallength; i++) {
                    var message = messages.messages[i]
                    var message_module = "";
                    if (message.event_id > lastId) {
                        message_module = "<a class='message-container' href='/announcements?a=" + message.event_id + "'><div class='notification-message new'>" + message.header + "</div></a>";
                    }
                    else {
                        message_module = "<a class='message-container' href='/announcements?a=" + message.event_id + "'><div class='notification-message'>" + message.header + "</div></a>";
                    }
                    $("#message_area").append(message_module);
                }
            }
            else {
                var message_module = "<div style='text-align: center; font-size: 14px; color: gray; padding-top: 10px'>Nothing to show!</div>";
                $("#message_area").append(message_module);
            }
            $("#notification_number").hide();
            showNotificationDropDown();
        }).fail(function (err) {
            console.log("ERROR: " + JSON.stringify(err));
        });
    }
});</script><script src="//d3js.org/d3.v3.min.js"></script><script src="//cdn.plot.ly/plotly-latest.min.js"></script><script src="/public/js/local/cards/dose_timepoint.js"></script><script src="/public/js/local/cards/introspect.js"></script><script src="/public/js/local/cards/class-celllines.js"></script><script src="/public/js/local/cards/class-heatmap.js"></script><script src="/public/js/local/cards/class-summary.js"></script><script src="/public/js/lib/d3.layout.chord.sort.js"></script><script src="/public/js/local/cards/cards.js"></script><script src="//cdnjs.cloudflare.com/ajax/libs/jquery.pjax/2.0.1/jquery.pjax.min.js"></script><script src="/public/js/lib/jquery-ui.min.js"></script><script src="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.2/js/select2.min.js" integrity="sha256-04G2Dnj+apKwEmFACpe+2vz/yh4YM6+FDQ2qhLyQX/s=" crossorigin="anonymous"></script><script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/js/bootstrap-datepicker.min.js"></script><link href="/public/css/lib/jquery-ui.min.css" rel="stylesheet"><link href="/public/css/local/introspect.css" rel="stylesheet"><link href="/public/css/local/class-card.css" rel="stylesheet"><link href="/public/css/local/cards/cards.css" rel="stylesheet"><script src="/public/js/lib/d3.min.js"></script><script src="/public/js/local/scarpa/d3notification.js"></script><script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.1/moment.min.js"></script><script src="/public/js/lib/d3.v4.min.js"></script><script src="/public/js/local/icv/diff.conn.js"></script><script src="//cdnjs.cloudflare.com/ajax/libs/echarts/3.8.5/echarts.min.js"></script><script src="/public/js/local/tablelegs/js/tablelegs.js"></script><script src="//cdn.plot.ly/plotly-latest.min.js"></script><script src="/public/js/local/data/buildSingleton.js"></script><script src="/public/js/local/data/clue.data.js"></script><script src="/public/js/local/utils/favorites.js"></script><script src="/public/js/local/data/prism.build.constants